{
  "name": "flipboxfactory/scorecard",
  "description": "Grade Craft CMS Elements",
  "version": "1.0.0-beta",
  "type": "craft-plugin",
  "license": "MIT",
  "authors": [{
    "name": "Flipbox Factory",
    "email": "hello@flipboxfactory.com"
  }],
  "require": {
    "craftcms/cms": "^3.0",
    "flipboxfactory/craft-ember": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2",
    "codeception/codeception": "^2.4",
    "vlucas/phpdotenv": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\scorecard\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\scorecard\\tests\\": "tests/src/"
    }
  },
  "extra": {
    "name": "Scorecard",
    "handle": "scorecard",
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "class": "flipbox\\scorecard\\Scorecard",
    "changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/scorecard/master/CHANGELOG.md",
    "downloadUrl": "https://github.com/flipboxfactory/scorecard/archive/master.zip",
    "hasCpSettings": false,
    "hasCpSection": false,
    "schemaVersion": "1.0.0",
    "branch-alias": {
      "dev-develop": "1.0.x-dev",
      "dev-master": "1.0.x-dev"
    }
  }
}
